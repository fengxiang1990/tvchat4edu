<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="IpSettingDialog" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="InMeetingQuitDialog" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="ContactUserActionDialog" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="InMeetingCameraSettingAnim">
        <item name="@android:windowEnterAnimation">@anim/nonam_scale_0_100</item>
        <item name="@android:windowExitAnimation">@anim/nonam_scale_100_0</item>
    </style>

    <style name="TitleBarPopupWindowAnim">
        <item name="@android:windowEnterAnimation">@anim/translation_down_to_up_related_self</item>
        <item name="@android:windowExitAnimation">@anim/translation_up_to_down_related_self</item>
    </style>

</resources>