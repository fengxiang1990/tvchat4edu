<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >
    <RelativeLayout
        android:id="@+id/RelativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff" >
        <include layout="@layout/common_activity_title_layout" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"
        android:orientation="vertical" >
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:background="#f2f2f2"
            android:orientation="vertical" >
            <include layout="@layout/common_contact_conversation_layout" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_left"
                android:layout_marginRight="@dimen/contact_detail_2_item_margin_right"
                android:background="@drawable/rounded_corners_of_contact_user_detail_outer"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_no_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_no"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:id="@+id/crowd_detail_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toRightOf="@+id/crowd_detail_no_tv"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_no"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#c0c0c0" >
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_creator_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_creator"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:id="@+id/crowd_detail_creator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toRightOf="@+id/crowd_detail_creator_tv"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:text="@string/crowd_invitation_box_item_creator"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#c0c0c0" >
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/crowd_detail_announcement_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_announcement_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_announcement"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:id="@+id/crowd_detail_announcement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toLeftOf="@+id/imageview1"
                        android:layout_toRightOf="@+id/crowd_detail_announcement_tv"
                        android:ellipsize="end"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:text="@string/crowd_invitation_box_item_announcement"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <ImageView
                        android:id="@+id/imageview1"
                        style="@style/Com_Arrow_Style"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginLeft="@dimen/contacts_group_view_arrow_padding_left"
                        android:src="@drawable/arrow_right_gray" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#c0c0c0" >
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/crowd_detail_brief_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_brief_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_brief"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:id="@+id/crowd_detail_brief"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toLeftOf="@+id/imageview2"
                        android:layout_toRightOf="@+id/crowd_detail_brief_tv"
                        android:ellipsize="end"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:text="@string/crowd_invitation_box_item_brief"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <ImageView
                        android:id="@+id/imageview2"
                        style="@style/Com_Arrow_Style"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginLeft="@dimen/contacts_group_view_arrow_padding_left"
                        android:src="@drawable/arrow_right_gray" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#c0c0c0" >
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/crowd_detail_invitation_members_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_members_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_member"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:id="@+id/crowd_detail_members"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toRightOf="@+id/crowd_detail_members_tv"
                        android:padding="10dp"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:layout_toRightOf="@+id/crowd_detail_members"
                        android:text="@string/crowd_invitation_box_item_member_unit"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <ImageView
                        style="@style/Com_Arrow_Style"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginLeft="@dimen/contacts_group_view_arrow_padding_left"
                        android:src="@drawable/arrow_right_gray" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_left"
                android:layout_marginRight="@dimen/contact_detail_2_item_margin_right"
                android:layout_marginTop="25dp"
                android:background="@drawable/rounded_corners_of_contact_user_detail_outer"
                android:orientation="vertical" >
                <RelativeLayout
                    android:id="@+id/crowd_detail_files_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_invitation_box_item_files"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                    <ImageView
                        android:id="@+id/crowd_detail_new_file_notificator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/crowd_detail_file_right_arrow_icon"
                        android:src="@drawable/red_nofificator" />
                    <ImageView
                        android:id="@+id/crowd_detail_file_right_arrow_icon"
                        style="@style/Com_Arrow_Style"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginLeft="@dimen/contacts_group_view_arrow_padding_left"
                        android:src="@drawable/arrow_right_gray" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/crowd_detail_admistrator_box"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                    android:layout_marginLeft="@dimen/contact_detail_2_item_margin_left"
                    android:layout_marginRight="@dimen/contact_detail_2_item_margin_right"
                    android:layout_marginTop="25dp"
                    android:orientation="vertical" >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:padding="10dp"
                        android:text="@string/crowd_detail_qulification_rule"
                        android:textColor="@color/activiy_contact_detail_item_color"
                        android:textSize="@dimen/common_item_text_size" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/crowd_detail_radio_group_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                    android:layout_marginLeft="@dimen/contact_detail_2_item_margin_left"
                    android:layout_marginRight="@dimen/contact_detail_2_item_margin_right"
                    android:background="@drawable/rounded_corners_of_contact_user_detail_outer"
                    android:orientation="vertical" >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp" >
                        <RadioGroup
                            android:id="@+id/crowd_detail_radio_group"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >
                            <RadioButton
                                android:id="@+id/radio0"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/common_radio_button_margin_buttom"
                                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginTop="@dimen/common_radio_button_margin_top"
                                android:button="@null"
                                android:checked="true"
                                android:drawableRight="@drawable/ws_common_btn_radio_selector"
                                android:padding="10dp"
                                android:tag="0"
                                android:text="@string/crowd_detail_qulification_rule_no_rule"
                                android:textColor="@color/activiy_contact_detail_item_color"
                                android:textSize="@dimen/common_item_text_size" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#c0c0c0" >
                            </LinearLayout>
                            <RadioButton
                                android:id="@+id/radio1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/common_radio_button_margin_buttom"
                                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginTop="@dimen/common_radio_button_margin_top"
                                android:button="@null"
                                android:drawableRight="@drawable/ws_common_btn_radio_selector"
                                android:padding="10dp"
                                android:tag="1"
                                android:text="@string/crowd_detail_qulification_rule_qulification"
                                android:textColor="@color/activiy_contact_detail_item_color"
                                android:textSize="@dimen/common_item_text_size" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#c0c0c0" >
                            </LinearLayout>
                            <RadioButton
                                android:id="@+id/radio2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/common_radio_button_margin_buttom"
                                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                                android:layout_marginTop="@dimen/common_radio_button_margin_top"
                                android:button="@null"
                                android:drawableRight="@drawable/ws_common_btn_radio_selector"
                                android:padding="10dp"
                                android:tag="2"
                                android:text="@string/crowd_detail_qulification_rule_never"
                                android:textColor="@color/activiy_contact_detail_item_color"
                                android:textSize="@dimen/common_item_text_size" />
                        </RadioGroup>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                android:layout_marginLeft="@dimen/contact_detail_2_item_margin_left"
                android:layout_marginRight="@dimen/contact_detail_2_item_margin_right"
                android:layout_marginTop="25dp"
                android:background="@drawable/rounded_corners_of_contact_user_detail_outer"
                android:orientation="vertical" >
                <LinearLayout
                    android:id="@+id/crowd_detail_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp" >
                    <TextView
                        android:id="@+id/crowd_detail_button_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/detail_detail_2_item_margin_bottom"
                        android:layout_marginLeft="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginRight="@dimen/contact_detail_2_item_margin_horizontal"
                        android:layout_marginTop="@dimen/detail_detail_2_item_margin_top"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="@string/crowd_detail_qulification_quit_button"
                        android:textColor="@color/common_item_text_color_red"
                        android:textSize="@dimen/common_title_text_size" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>